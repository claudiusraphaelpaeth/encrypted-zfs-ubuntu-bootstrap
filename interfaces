auto lo
iface lo inet loopback

iface eth0 inet manual

iface wlan0 inet dhcp
    pre-up wpa_supplicant -B -i $IFACE -Dnl80211 -p/run/wpa_supplicant-$IFACE -c/etc/wpa_supplicant.conf
    post-down kill $(cat /run/wpa_supplicant-$IFACE)

